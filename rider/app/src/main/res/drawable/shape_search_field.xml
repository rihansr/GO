<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="@dimen/margin_Small"
        android:bottom="@dimen/margin_Small">
        <shape android:shape="rectangle">
            <solid android:color="#05FFFFFF"/>
            <corners android:radius="@dimen/radius_X_Small"/>
        </shape>
    </item>

    <item
        android:left="@dimen/border_Small"
        android:right="@dimen/border_Small"
        android:top="@dimen/_5sdp"
        android:bottom="@dimen/_5sdp">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/border_Small" android:color="#0D5E5E5E"/>
            <solid android:color="@color/colorTrans"/>
            <corners android:radius="@dimen/radius_X_Small"/>
        </shape>
    </item>
</layer-list>